{
    "rewrites": [
        {
            "source": "/api/youdao/:match*",
            "destination": "https://dict.youdao.com/:match*"
        },
        {
            "source": "/api/deepseek/:match*",
            "destination": "https://api.deepseek.com/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}